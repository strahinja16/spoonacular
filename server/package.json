  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.11",
    "@types/redis": "^2.8.14",
    "@types/sequelize": "4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
